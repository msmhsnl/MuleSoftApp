    <db:generic-config name="Generic_Database_Configuration" url="jdbc:sqlserver://${db.port};database=${db.database};user=${db.user};password=${db.password};" driverClassName="com.microsoft.sqlserver.jdbc.SQLServerDriver" doc:name="Generic Database Configuration"/>
    <context:property-placeholder location="project-${env}.properties"/>

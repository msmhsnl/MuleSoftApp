{
	"type": "object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"required": true,
	"properties": {
		"OrderCode": {
			"type": "integer",
			"required": true
		},
		"Items": {
			"type": "array",
			"minItems": 0,
			"required": true,
			"items": {
				"type": "object",
				"required": true,
				"properties": {
					"ProductCode": {
						"type": "integer",
						"required": true
					},
					"Quantity": {
						"type": "integer",
						"required": true
					},
					"TotalAmount": {
						"type": "integer",
						"required": true
					}
				}
			}
		}
	}
}